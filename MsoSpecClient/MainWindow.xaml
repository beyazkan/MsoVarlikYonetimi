            <WrapPanel DockPanel.Dock="Left">
                <MsoControlLibrary:Computer Width="100"/>
                <MsoControlLibrary:Computer Width="100"/>
                <MsoControlLibrary:Computer Width="100"/>
                <MsoControlLibrary:Computer Width="100"/>
                <MsoControlLibrary:Computer Width="100"/>
            </WrapPanel>
        </DockPanel>
    </Grid>
</Window>
